    "class Point:\n",
    "    x = 4\n",
    "    y = 3\n",
    "\n",
    "    def get_distance_from_origin(self):\n",
    "        return (self.x**2 + self.y**2)**0.5\n",
    "\n",
    "p = Point()\n",
    "print(p.x, p.y) # 4 3\n",
    "print(p.get_distance_from_origin()) # 5.0\n"
   ]
  }
